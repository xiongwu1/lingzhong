-- Create table
create table T_ZL_ZD
(
  dm              VARCHAR2(20),
  mc              VARCHAR2(200),
  jc              VARCHAR2(100),
  sm              VARCHAR2(500),
  pyjp            VARCHAR2(100),
  lxjp            VARCHAR2(50),
  sourceid        VARCHAR2(50),
  yxx             VARCHAR2(1),
  px              NUMBER,
  sjdm            VARCHAR2(20),
  imageurl        VARCHAR2(200)
);
/**
tablespace ZL_DATA0
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64
    next 1
    minextents 1
    maxextents unlimited
  );**/
-- Add comments to the table 
comment on table T_ZL_ZD
  is '众邻-字典表';
-- Add comments to the columns 
comment on column T_ZL_ZD.dm
  is '代码';
comment on column T_ZL_ZD.mc
  is '名称';
comment on column T_ZL_ZD.jc
  is '简称';
comment on column T_ZL_ZD.sm
  is '说明';
comment on column T_ZL_ZD.pyjp
  is '拼音简拼';
comment on column T_ZL_ZD.lxjp
  is '类型简拼';
comment on column T_ZL_ZD.sourceid
  is '源代码';
comment on column T_ZL_ZD.yxx
  is '有效性';
comment on column T_ZL_ZD.px
  is '排序字段';
comment on column T_ZL_ZD.sjdm
  is '上级代码';
comment on column T_ZL_ZD.imageurl
  is '图像地址';
